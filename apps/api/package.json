{
  "name": "flowweek-api",
  "version": "1.0.0",
  "description": "Backend API for FlowWeek",
  "main": "dist/index.js",
  "installConfig": {
    "hoist": false
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "echo 'No linting configured'",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "prisma migrate deploy",
    "db:dev": "prisma migrate dev",
    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "prisma": "^6.16.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
